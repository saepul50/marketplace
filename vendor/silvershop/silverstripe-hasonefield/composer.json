{
    "name": "silvershop/silverstripe-hasonefield",
    "description": "Edit a has_one relationship using a GridField, triggered by clicking a button.",
    "license": "BSD-2-Clause",
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/vendor-plugin": "^1.0 || ^2.0",
        "silverstripe/framework": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SilverShop\\HasOneField\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "hchokshi/silverstripe-hasonefield": "*"
    }
}
